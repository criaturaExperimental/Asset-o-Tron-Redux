<p id="notice"><%= notice %></p>

<h1>Listing My Assets</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @my_assets.each do |my_asset| %>
      <tr>
        <td><%= my_asset.name %></td>
        <td><%= link_to 'Show', my_asset %></td>
        <td><%= link_to 'Edit', edit_my_asset_path(my_asset) %></td>
        <td><%= link_to 'Destroy', my_asset, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New My asset', new_my_asset_path %>
