<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @project.name %>
</p>
<table>
  <thead>
    <tr>
      <th>Asset name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @project.my_assets.each do |my_asset| %>
      <tr>
        <td><%= my_asset.name %></td>
        <td><%= link_to 'Show', [@project, my_asset] %></td>

        <td><%= link_to 'Destroy', [@project, my_asset], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'Edit', edit_project_path(@project) %> |
<%= link_to 'Back', projects_path %>
